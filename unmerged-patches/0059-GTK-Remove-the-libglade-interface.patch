-static void init_pix_col_map() {
-	// precalc factors
-	int a,b,c,A,B,C,rA,rB,rC;
-	if (has_pix_col_map) return;
-	for (a=0; a<0x20; a++) {
-		A=a<<10; rA=A<<9;
-		for (b=0; b<0x20; b++) {
-			B=b<<5; rB=B<<6;
-			for (c=0; c<0x20; c++) {
-				C=c; rC=C<<3;
-				pix_col_map[A|B|C]=rA|rB|rC;
-			}
-		}
-	}
-	has_pix_col_map=1;
-}
